proc(_chat_intercept,@player,	assign(@time,time())	if(has_permission(@player,'hermitcraft.hermit'),		return(false)	)	assign(@stored_time,get_value(to_lower(@player)))	if(@stored_value == 'null',		store_value(to_lower(@player),@time),		if(@time - @stored_time > 30000,			store_value(to_lower(@player),@time)			assign(@cancel,false),			msg(concat(color('red'),'You can only chat once every 30 seconds'))			assign(@cancel,true)		)	)	return(@cancel))