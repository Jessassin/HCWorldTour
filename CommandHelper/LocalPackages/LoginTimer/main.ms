bind(player_prelogin,null,null,@event,	assign(@player,@event['player'])	assign(@cancel,_filter_login(@player))	if(@cancel == 'true',		modify_event('result','KICK_OTHER')		modify_event('kickmsg','You ran out of time to play :(')	))bind(player_join,null,null,@event,	assign(@player,@event['player'])  runas(@player,'/speed walk 2')  assign(@l_player,to_lower(@player))  assign(@firstlogin,get_value(concat(@l_player,'_first_login')))  if(@firstlogin == null,    console('First login is null!')    _set_first_login(@player)  ))_interval()