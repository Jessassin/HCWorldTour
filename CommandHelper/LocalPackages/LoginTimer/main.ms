bind(player_prelogin,null,null,@event,	assign(@player,@event['player'])	assign(@cancel,_filter_login(@player))	if(@cancel == 'true',		modify_event('result','KICK_OTHER')		modify_event('kickmsg','You ran out of time to play :(')	))bind(player_login,null,null,@event,	assign(@player,@event['player'])  queue_push(closure(    runas(@player,'/speed walk 2')  )))_interval()